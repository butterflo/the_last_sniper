    <!-- Nuke                                                                                    -->
    <CActorSplat id="nukeCursorSplat">
        <On index="0" Terms="Abil.Snipe.SourceCastStart"/>
        <On index="1" Terms="ActorDestruction" Target="CursorRegion" Send="Destroy"/>
        <On index="2" Terms="ActorOrphan" Target=""/>
        <On index="3" Terms="Effect.SnipeDamage"/>
        <On Terms="Abil.Snipe.SourceCastStop" Send="Destroy"/>
        <On Terms="Abil.NovaSnipe.SourceCastStart" Send="Create"/>
        <On Terms="Abil.NovaSnipe.SourceCastStop" Send="Destroy"/>
        <On Terms="Abil.NovaSnipe.SourceCastStart" Send="SetScale 4.000000 4.000000"/>
        <On Terms="Effect.NovaSnipe" Send="Destroy"/>
    </CActorSplat>
