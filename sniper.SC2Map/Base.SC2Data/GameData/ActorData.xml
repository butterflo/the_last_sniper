    <!--                                                                                         -->
    <!-- Ghost ................................................................................. -->
    <!--                                                                                         -->
    <CActorUnit id="Ghost">
        <Scale value="0.500000,0.500000,0.100000"/>
        <StatusBarFlags index="Shields" value="0"/>
        <StatusBarFlags index="Life" value="0"/>
        <StatusBarFlags index="Energy" value="0"/>
        <StatusBarFlags index="Cargo" value="0"/>
        <StatusBarFlags index="Duration" value="0"/>
        <On index="45" Terms="Abil.Snipe.SourceCastStart" Send="AnimBaselineStop"/>
        <On index="46" Terms="Abil.Snipe.SourceCastStart" Send="AnimPlay SpellA Spell,A"/>
        <On index="47" Terms="Abil.Snipe.SourceCastStop" Send="AnimClear SpellA"/>
        <On index="48" Terms="Abil.Snipe.SourceCastStop" Send="AnimBaselineStart"/>
        <On index="49" Terms="Abil.Snipe.SourceChannelStart" Send="AnimPlay SpellB Spell,B PlayForever"/>
        <On index="50" Terms="Abil.Snipe.SourceChannelStop" Send="AnimClear SpellB"/>
        <On index="51" Terms="Abil.Snipe.SourceFinishStart" Send="AnimPlay SpellC Spell,C"/>
        <On index="52" Terms="UnitMovementUpdate.*.Walk; AnimPlaying SpellC" Send="AnimClear SpellC"/>
        <On index="53" Terms="Abil.EMP.SourceCastStart" Send="AnimPlay SpellC Spell,C 0 0.000000 0.300000"/>
        <On index="54" Terms="Abil.attack.ReadyStart" Send="AnimGroupApply Ready"/>
        <On index="55" Terms="Behavior.GhostHoldFireB.On" Send="AnimGroupApply Cover"/>
        <On index="56" Terms="Behavior.GhostHoldFireB.On" Send="AnimPlay MorphStart Cover,Start"/>
        <On index="57" Terms="Behavior.GhostHoldFireB.On" Send="AnimClear MorphEnd"/>
        <On index="58" Terms="Behavior.GhostHoldFireB.Off" Send="AnimPlay MorphEnd Cover,End"/>
        <On index="59" Terms="Behavior.GhostHoldFireB.Off" Send="AnimClear MorphStart"/>
        <On index="60" Terms="Behavior.GhostHoldFireB.Off" Send="AnimGroupRemove Cover"/>
        <On index="61" Terms="WeaponStart.*.AttackStart; WeaponTargetElevation GE 20.000000" Send="AnimBracketStart Attack Attack,Superior"/>
        <On index="62" Terms="WeaponStart.*.AttackStart; WeaponTargetElevation LE -20.000000" Send="AnimBracketStart Attack Attack,Inferior"/>
        <On index="63" Terms="WeaponStart.*.AttackStart" Send="AnimBracketStart Attack Attack"/>
        <On index="64" Terms="WeaponStop.*.AttackStop" Send="AnimBracketStop Attack"/>
        <On index="65" Terms="Abil.attack.ReadyStop" Send="AnimGroupRemove Ready"/>
        <On index="66" Terms="Abil.NovaSnipe.SourceCastStart"/>
        <On index="67" Terms="Abil.NovaSnipe.SourceCastStart"/>
        <On index="68" Terms="Abil.NovaSnipe.SourceCastStop"/>
        <On index="69" Terms="Abil.NovaSnipe.SourceCastStop"/>
        <On index="70" Terms="Abil.NovaSnipe.SourceChannelStart"/>
        <On index="71" Terms="Abil.NovaSnipe.SourceChannelStop"/>
        <On Terms="Abil.NovaSnipe.SourceFinishStart" Send="AnimPlay SpellC Spell,C"/>
        <On Terms="Signal.*.GlowRed" Send="HaloSetColor 255,0,0"/>
        <On Terms="Signal.*.GlowBlue" Send="HaloSetColor 0,0,255"/>
        <On Terms="Signal.*.GlowSet" Send="HaloStart"/>
        <ModelFlags index="OutlineEmitter" value="0"/>
    </CActorUnit>
    <CActorAction id="GhostEMPAttack"/>
    <CActorModel id="GhostEMPAttackImpactEffect">
        <On Terms="ActorCreation" Send="SetTintColor 255,255,128"/>
